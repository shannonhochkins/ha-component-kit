{
  "name": "Hakit",
  "version": "1.1.0",
  "slug": "hakit",
  "init": false,
  "ingress": true,
  "ingress_stream": true,
  "stage": "stable",
  "url": "https://github.com/shannonhochkins/ha-component-kit",
  "changelog": "https://github.com/shannonhochkins/ha-component-kit/blob/main/hakit/CHANGELOG.md",
  "ingress_port": 2022,
  "description": "An addon to render custom dashboards for home assistant using @hakit.",
  "arch": ["aarch64", "amd64", "armv7"],
  "boot": "auto",
  "map": [
    "config:rw",
    "share:rw",
    "backup:rw",
    "media:rw",
    "ssl:ro"
  ],
  "options": {
    "html_file_path": "www/ha-dashboard/index.html",
    "spa_mode": true
  },
  "schema": {
    "html_file_path": "str",
    "spa_mode": "bool"
  },
  "startup": "services",
  "hassio_api": true,
  "homeassistant_api": true,
  "panel_admin": true,
  "panel_icon": "mdi:tablet-dashboard",
  "panel_title": "HAKIT Dashboard"
}